{
  "name": "cyberpong-backend",
  "version": "1.3",
  "description": "The Best Pong Tournament Website (we host)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migrate": "npm run typeorm -- -d ./src/typeorm/data-source.ts migration:generate ./src/typeorm/migration/$npm_config_name",
    "typeorm:run": "npm run typeorm -- -d ./src/typeorm/data-source.ts migration:run",
    "typeorm:revert": "npm run typeorm -- -d ./src/typeorm/data-source.ts migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/testing": "^10.3.8",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.8",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.12.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-typeorm": "^2.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^9.3.0",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-42": "^1.2.6",
    "pg": "^8.7.3",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@types/jest": "^29.5.12",
    "@types/passport-discord": "^0.1.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.76.0"
  },
  "_moduleAliases": {
    "src": "./src"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
